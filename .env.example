# OpenAI API Configuration
OPENAI_API_KEY=your-api-key-here
OPENAI_ORG_ID=your-org-id-here  # Optional

# Model Configuration
MODEL_NAME=gpt-5.1
MAX_COMPLETION_TOKENS=4096
TEMPERATURE=0.7

# Paths
PROJECT_DIR=/path/to/adaptive-swe-agent
DATA_DIR=/path/to/data
CACHE_DIR=/tmp/adaptive-swe-cache

# Performance
PARALLEL_WORKERS=4
ENABLE_CACHING=true

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/adaptive_agent.log